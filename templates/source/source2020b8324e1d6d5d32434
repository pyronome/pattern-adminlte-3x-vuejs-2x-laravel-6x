import HomePage from "./components/HomePage.vue";
import ProfilePage from "./components/ProfilePage.vue";
import ServerInformationPage from "./components/ServerInformationPage.vue";

var mainFolder = document.body.getAttribute("data-main-folder");
if (!mainFolder) {
    mainFolder = "adminlte";
}

export var Routes = [
    {
        "path": ("/" + mainFolder + "/"),
        "component": HomePage
    },
    {
        "path": ("/" + mainFolder + "/home"),
        "component": HomePage
    },
    {
        "path": ("/" + mainFolder + "/profile"),
        "component": ProfilePage
    },
    {
        "path": ("/" + mainFolder + "/server_information"),
        "component": ProfilePage
    }
];